        "transform = transforms.Compose([\n",
        "    transforms.ToTensor(),\n",
        "    transforms.Lambda(lambda x: x * 2.0 - 1.0)\n",
        "])\n",
        "\n",
        "batch_size = 128\n",
        "epoch = 10\n",
        "lr = 2e-4\n",
        "beta_schedule = 'linear'\n",
        "beta_start = 0.0001\n",
        "beta_end = 0.02\n",
